{
  "name": "capm_ts",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.2",
    "@cap-js/hana": "^2.1.2",
    "@dxfrontier/cds-ts-dispatcher": "^5.0.1",
    "@dxfrontier/cds-ts-repository": "^6.0.1",
    "@sap-cloud-sdk/connectivity": "^4.1.1",
    "@sap-cloud-sdk/http-client": "^4.1.1",
    "@sap-cloud-sdk/resilience": "^4.1.1",
    "@sap/cds": "9.1.0",
    "@sap/cds-dk": "^9.2.0",
    "cdse": "^1.1.12",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.36.0",
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^2.0.2",
    "@sap/cds-dk": ">=8",
    "@types/node": "^22.17.0",
    "cds-plugin-ui5": "^0.13.0",
    "typescript": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "build": "cds build",
    "build:mta": "mbt build",
    "build:module": "npx @cap-js/cds-typer '*' --outputDirectory @cds-models",
    "build:deploy": "mbt build && cf deploy mta_archives/capm_ts_1.0.0.mtar",
    "watch-book_shop1": "cds watch --open bookshop1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy capm_ts --delete-services --delete-service-keys --delete-service-brokers",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch": "npx cds-tsx w",
    "watch-demo_ui5": "cds watch --open demoui5/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "plugins": [],
    "requires": {
      "auth": {
        "kind": "mocked"
      },
      "destinations": true,
      "html5-runtime": true,
      "db": {
        "[development]": {
          "kind": "sqlite"
        },
        "[production]": {
          "kind": "hana"
        }
      },
      "AdminService": {
        "kind": "odata",
        "model": "srv/external/AdminService/AdminService",
        "credentials": {
          "destination": "invoflow_capm_srv",
          "path": "/odata/v2/admin"
        },
        "[production]": {
          "credentials": {
            "destination": "invoflow_capm_srv",
            "path": "/odata/v2/admin"
          }
        }
      },
      "NorthWind": {
        "kind": "odata",
        "model": "srv/external/NorthWind/NorthWind",
        "credentials": {
          "destination": "NorthWind"
        },
        "[production]": {
          "credentials": {
            "destination": "NorthWind"
          }
        }
      }
    }
  },
  "imports": {
    "#dispatcher": "./@dispatcher/index.js",
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "workspaces": [
    "app/*"
  ]
}
